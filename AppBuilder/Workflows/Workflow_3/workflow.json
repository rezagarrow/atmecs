            "Value": "var data = JSON.parse(result).daily\n\nfunction tc(valNum) {\n  valNum = parseFloat(valNum);\n  return (((valNum-273.15)*1.8)+32).toFixed(1);\n}\n\nvar response = data.map(function(o){\nreturn {'date':new Date(o.dt * 1000).toDateString(), 'weather': `${o.weather[0].main} (${o.weather[0].description})`, 'day': `${tc(o.temp.day)}째F`, 'morn': `${tc(o.temp.morn)}째F`, 'eve': `${tc(o.temp.eve)}째F`, 'night': `${tc(o.temp.night)}째F`, 'windSpeed': `${o.wind_speed} miles`} \n})\n\nreturn JSON.stringify(response);",
